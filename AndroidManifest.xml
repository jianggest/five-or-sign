<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.five_or_sign.five_or_sign">
    <uses-sdk android:minSdkVersion="8"/>
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:theme="@style/buttonStyle">
        <activity android:name=".Main"
                  android:label="@string/app_name" android:screenOrientation="portrait" android:icon="@drawable/icon">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".About" android:screenOrientation="portrait" android:icon="@drawable/icon"></activity>
        <activity android:screenOrientation="portrait" android:name=".Help" android:icon="@drawable/icon"></activity>
        <activity android:name=".NewGame" android:screenOrientation="portrait"></activity>
        <activity android:name=".Game" android:screenOrientation="portrait"></activity>
        <activity android:name=".Winner" android:screenOrientation="portrait"></activity>
    </application>
</manifest>
